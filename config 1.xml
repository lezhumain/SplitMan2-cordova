<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.splitman2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
<!--    <hook type="before_prepare" src="scripts/prebuild.js" />-->
    <name>SplitMan2</name>
    <description>Sample Apache Cordova App</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidExtraFilesystems" value="files,cache,sdcard,cache-external,files-external" />
	<variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
</widget>
